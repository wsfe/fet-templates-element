{
  "name": "fet-iview-temp",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "freefish",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --port 8080",
    "build": "vue-cli-service build --server=online",
    "build:beta": "vue-cli-service build --server=beta",
    "build:report": "vue-cli-service build --report",
    "pack": "vue-cli-service build --mode joint",
    "pack:dev1": "npm run pack --server=dev1",
    "sync:dev1": "npm run pack:dev1 && fet sync dev1",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --require tests/unit/index.js"
  },
  "dependencies": {
    "axios": "0.18.0",
    "codemirror": "^5.52.2",
    "core-js": "^3",
    "dayjs": "^1.8.18",
    "driver.js": "^0.9.8",
    "dropzone": "^5.7.0",
    "echarts": "^4.7.0",
    "element-ui": "^2.13.1",
    "fuse.js": "^5.1.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "screenfull": "^5.0.2",
    "script-loader": "^0.7.2",
    "sortablejs": "^1.10.2",
    "tui-editor": "^1.4.10",
    "vue-count-to": "^1.0.13",
    "vue-router": "^3.1.2",
    "vue-splitpane": "^1.0.6",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-unit-mocha": "^4.1.2",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-standard": "^5.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "faker": "^4.1.0",
    "lint-staged": "^9.5.0",
    "minimist": "^1.2.0",
    "mockajax": "^1.1.7",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "sinon": "^8.0.2",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.4.0"
  }
}
